# -------------------------------------------------------------------------
#	clean_Semantique
# -------------------------------------------------------------------------

.PHONY : clean_Semantique

clean_Semantique :
	@echo "# -------------------------------------------------------------------------"
	@echo "--> $@:"
	@echo "# -------------------------------------------------------------------------"
	@echo ""
	rm -f \
		$(FICHIERS_SYNTHETISES_Semantique) \
		$(OBJETS_Semantique) \
		$(EXECUTABLES_Semantique)
	@echo ""
