# -------------------------------------------------------------------------
#	La cible à construire
# -------------------------------------------------------------------------

.PHONY : message_Semantique executables_Semantique

message_Semantique:
	@echo "# -------------------------------------------------------------------------"
	@echo "--> objets_Semantique:"
	@echo "# -------------------------------------------------------------------------"
	@echo ""

executables_Semantique: \
		message_Semantique \
		$(EXECUTABLES_Semantique)


# -------------------------------------------------------------------------
#	Compilation de la grammaire par Bison
# -------------------------------------------------------------------------

$(SEMANTIQUE)/Semantique.tab.cc \
$(SEMANTIQUE)/Semantique.tab.hh : \
		Semantique.Bison \
		$(MAKEFILE)
	@echo ""
	bison \
		-o $@ \
		--defines --verbose --token-table --debug --yacc \
		$<
	@echo ""


# -------------------------------------------------------------------------
#	LanguesSemantique
# -------------------------------------------------------------------------

$(SEMANTIQUE)/LanguesSemantique.o : \
		$(SEMANTIQUE)/LanguesSemantique.cc \
		$(SEMANTIQUE)/LanguesSemantique.h \
		$(TYPES_ET_VERSION)/Types.h \
		$(SUPPORT_CPLUSPLUS)/SupportCPlusPlus.h \
		$(LANGUES_PREDEFINIS)/LanguesPredefinis.h \
		$(LEXIQUE)/LanguesLexique.h \
		$(SYNTAXE)/LanguesSyntaxe.h \
		$(MAKEFILE_Semantique)
	$(CXX) \
		-c $< \
		-o $@ \
		$(OPTIONS_CXX) \
			-I $(TYPES_ET_VERSION) \
			-I $(SUPPORT_CPLUSPLUS) \
			-I $(LANGUES_ET_DECODAGE_D_OPTIONS) \
			-I $(LANGUES_PREDEFINIS) \
			-I $(LEXIQUE) \
			-I $(SYNTAXE)
	@echo ""


$(SEMANTIQUE)/LanguesSemantique_SYNTHESE_PILUM.o : \
		$(SEMANTIQUE)/LanguesSemantique.cc \
		$(SEMANTIQUE)/LanguesSemantique.h \
		$(TYPES_ET_VERSION)/Types.h \
		$(SUPPORT_CPLUSPLUS)/SupportCPlusPlus.h \
		$(LANGUES_PREDEFINIS)/LanguesPredefinis.h \
		$(LEXIQUE)/LanguesLexique.h \
		$(SYNTAXE)/LanguesSyntaxe.h \
		$(MAKEFILE_Semantique)
	$(CXX) \
		-c $< \
		-o $@ \
		$(OPTIONS_CXX) \
		-DSYNTHESE_PILUM \
			-I $(TYPES_ET_VERSION) \
			-I $(SUPPORT_CPLUSPLUS) \
			-I $(LANGUES_ET_DECODAGE_D_OPTIONS) \
			-I $(LANGUES_PREDEFINIS) \
			-I $(LEXIQUE) \
			-I $(SYNTAXE) \
			-I $(PILUM) \
			-I $(SYNTHESE_PILUM)
	@echo ""


